			"IngeritanceDemo/Ownable.sol": {
				"content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.5.0; \n\ncontract Ownable {\n    address owner;\n\n    constructor() public  {\n        owner = msg.sender;\n    }\n\n    modifier onlyOwner(){\n        require(msg.sender == owner, \"Must be owner\");\n        _;\n    }\n}"
			}
